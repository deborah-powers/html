{
	"manifest_version": 2,
	"name": "we-toggle",
	"description": "exemple de web-extension affichant ou masquant du css",
	"version": "1.0",
	"permissions": [ "activeTab", "tabs" ],
	"icons": { "128" : "icon.svg" },
	// lancer une we sans popup. je peux aussi utiliser browser_action
	"page_action": {
		"default_icon" : "icon.svg",
		"default_title": "we-toggle"
	},
	"background": { "scripts": [ "toggle.js" ] }
}